{
  "name": "clutter-ui-vue",
  "description": "A UI for Clutter based on Vue",
  "author": "Jost Schulte",
  "version": "0.0.1",
  "scripts": {
    "prepare-env": "echo 'VITE_HC_PORT='$HC_PORT > .env.local",
    "start": "npm run prepare-env && vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ext .js,.ts,.vue"
  },
  "dependencies": {
    "@holochain/client": "^0.3.2",
    "@quasar/extras": "^1.12.4",
    "quasar": "^2.4.12",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^8.7.0",
    "eslint-plugin-vue": "^8.4.0",
    "sass": "^1.32.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
